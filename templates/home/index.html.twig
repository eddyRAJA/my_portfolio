{% extends 'base.html.twig' %}

{% block title %}AboutMe!{% endblock %}

{% block body %}
  <div class="text-center">
    <img src="{{ asset('build/images/logo.png') }}" alt=""/>
  </div>
  <div class="text-center">
    <h1>
      {{ aboutMe.function }}
    </h1 >
    <h4>
      {{ aboutMe.title }}
    </h4>
    <p>
      {{ aboutMe.email }}
    </p>
    <p>
      {{ aboutMe.githubLink }}
    </p>
    <p>
      <img src="{{ aboutMe.avatar }}" alt=""/>
    </p>
    <p>
      {{ aboutMe.description|raw }}
    </p>
  </div>
{% endblock %}
